export SVN_EDITOR="emacs -nw"

export TERM=xterm-color
export HOSTTYPE=Linux
usernames=( $(cut -d: -f1 /etc/passwd) )
groups=( $(cut -d: -f1 /etc/group) )


if [ -f ~/.zsh/hosts ] ; then
    hostnames=( `cat ~/.zsh/hosts` )
else
    hostnames=( localhost `hostname -s` )
fi

case "$TERM" in
    xterm*|rxvt|linux)
	;;
    *)
	nocolor=yes
	;;
esac # um like take this out if you dont use this in /etc or something i guess.
if [ -f ~/.dircolors ] ; then
    eval "`dircolors ~/.dircolors`"
elif [ -f /etc/DIR_COLORS ] ; then
    eval `dircolors /etc/DIR_COLORS`
fi

export LESSCHARSET=latin1
export MINICOM="-c on"
export LESS="-S -g"
LOGCHECK=60
WATCHFMT="%n has %a %l from %M"
export HISTFILE=$HOME/.zshhistory
export HISTSIZE=5000
export SAVEHIST=3000
export LISTMAX=0
export COLORTERM=vt100-color
export NNTPSERVER=news.epita.fr
export PAGER=more
export FULLNAME="Vincent CUISSARD"
export BLOCK_SIZE=human-readable
export LS_COLORS='no=00:fi=00:di=0;33:ln=01;36:pi=40;33:so=01;35:bd=40;33;01:cd=40;33;01:or=40;31;01:ex=01;32:*.tar=01;31:*.tgz=01;31:*.arj=01;31:*.taz=01;31:*.lzh=01;31:*.zip=01;31:*.z=01;31:*.Z=01;31:*.gz=01;31:*.bz2=01;31:*.deb=01;31:*.rpm=01;31:*.jpg=01;35:*.gif=01;35:*.bmp=01;35:*.pgm=01;35:*.pbm=01;35:*.ppm=01;35:*.tga=01;35:*.png=01;35:*.GIF=01;35:*.JPG=01;35:*.xbm=01;35:*.xpm=01;35:*.tif=01;35:*.mpg=01;37:*.avi=01;37:*.gl=01;37:*.dl=01;37:*.mly=01;37:*.mll=01;37:*.mli=01;37:*.ml=01;37:*.cpp=01;37:*.cc=01;37:*.c=01;37:*.hh=01;37:*.h=01;37:*Makefile=4;32:*.pl=4;32:*.sh=4;32:*.ps=01;34:*.pdf=01;34:*TODO=01;37:*TOGO=01;37:*README=01;37:*LINKS=01;37:*.y=01;37:*.l=01;37:*.algo=01;37'

export CVS_RSH=`which ssh`
#limit stacksize 64M
limit core 0
